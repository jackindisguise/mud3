{
	"name": "mud3",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"retest": "npm run build && npm run test",
		"test": "npm run test:compiled",
		"test:compiled": "node --test --test-reporter=spec \"dist/**/*.spec.js\"",
		"test:raw": "node --loader ts-node/esm --test --test-reporter=spec \"src/**/*.spec.ts\"",
		"coverage": "npx c8 --reporter=html --reporter=text-summary npm run test",
		"deploy": "npm run build && npm run test && npm run coverage",
		"doc": "npx typedoc",
		"start": "node dist/index",
		"rerun": "npm run build && npm run start"
	},
	"devDependencies": {
		"@types/ini": "^4.1.1",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.9.1",
		"@types/xml2js": "^0.4.14",
		"c8": "^10.1.3",
		"js-yaml": "^4.1.0",
		"mud-ext": "github:jackindisguise/mud-ext",
		"rimraf": "^6.0.1",
		"smol-toml": "^1.4.2",
		"ts-node": "^10.9.2",
		"typedoc": "^0.28.14",
		"typescript": "^5.9.3",
		"winston": "^3.18.3"
	},
	"dependencies": {
		"package-loader": "github:jackindisguise/package-loader"
	}
}
